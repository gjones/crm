%p#notice= notice
%h1 Notes
%table
  %thead
    %tr
      %th Body
      %th Author
      %th{:colspan => "3"}
  %tbody
    - @notes.each do |note|
      %tr
        %td= note.body
        %td= note.author
        %td= link_to 'Show', contact_note_path(note.contact_id, note.id)

%br/

= link_to 'Back', all_notes_path
